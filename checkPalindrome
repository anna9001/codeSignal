boolean checkPalindrome(String inputString) {
    
   
    
		String[] s = inputString.split("");

		for(int i=0; i<s.length/2;i++){

			for(int j=s.length-1;j>=s.length/2;j--){
                
				if(s[i].equals(s[j])){
                    
					i++;

				}else{
					return false; 
				}//for j

			} //for i

		}

    return true;

}
